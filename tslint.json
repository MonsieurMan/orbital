{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "interface-name": false,
        "ordered-imports": false,
        "member-access": false,
        "quotemark": {
            "options": "single"
        },
        "object-literal-sort-keys": false,
        "no-console": false,
        "arrow-parens": false
    },
    "rulesDirectory": []
}